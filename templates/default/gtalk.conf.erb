[general]
context=google-in		; Context to dump call into
allowguest=yes

[guest]			; special account for options on guest account
disallow=all
allow=ulaw

<% if node['asterisk']['auth'] %>
[gtalk]
username=<%= node['asterisk']['auth']['username'] %>
disallow=all
allow=ulaw
context=google-in
connection=google
<% end %>
